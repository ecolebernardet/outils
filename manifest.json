{
  "manifest_version": 3,
  "name": "Speed Dial Perso",
  "short_name": "SpeedDial",
  "version": "2.9",
  "description": "Mon nouvel onglet personnalis√© avec gestion de dossiers.",
  "permissions": ["storage"],
  
  "action": {
    "default_popup": "index.html"
  },

  "background": {
    "service_worker": "sw.js" 
  },

  "icons": {
    "512": "https://cdn-icons-png.flaticon.com/512/3588/3588612.png"
  },

  "start_url": "index.html",
